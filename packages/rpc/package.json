{
  "name": "@chainflip/rpc",
  "version": "1.4.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "prepublish": "pnpm test run && pnpm build:clean",
    "build": "tsup",
    "build:clean": "pnpm clean && pnpm build",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chainflip/utils": "^0.1.3",
    "zod": "^3.22.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/ws": "^8.5.10",
    "ws": "^8.17.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    "./*": {
      "require": "./dist/*.cjs",
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
