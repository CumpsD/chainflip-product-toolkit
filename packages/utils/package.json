{
  "name": "@chainflip/utils",
  "version": "0.1.2",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "prepublish": "pnpm test run && pnpm build:clean",
    "build": "tsup",
    "build:clean": "pnpm clean && pnpm build",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    "./*": {
      "require": {
        "default": "./dist/*.cjs",
        "types": "./dist/*.d.cts"
      },
      "import": {
        "default": "./dist/*.js",
        "types": "./dist/*.d.ts"
      }
    }
  },
  "devDependencies": {
    "@noble/hashes": "^1.4.0"
  }
}
